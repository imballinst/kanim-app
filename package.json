{
  "name": "kanim",
  "version": "1.0.0",
  "description": "aplikasi untuk notifikasi antrian kanim",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "restart": "pm2 restart kanim-app",
    "stop": "pm2 stop kanim-app --watch",
    "delete": "pm2 delete kanim-app",
    "log": "pm2 logs kanim-app",
    "dev": "npm start && npm run log",
    "show": "pm2 show kanim-app",
    "test": "jest && cross-env eslint --fix app.js lib/. bin/www routes/.",
    "test:watch": "jest --watch",
    "db:seed": "cross-env APPLY_SEED=true node ./db/seeder.js",
    "db:clean": "node ./db/seeder.js"
  },
  "author": "Try Ajitiono",
  "license": "MIT",
  "keywords": [
    "kanim",
    "notifikasi"
  ],
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$",
    "moduleFileExtensions": [
      "js"
    ],
    "setupFiles": [
      "./scripts/JestSetup.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cron": "^1.3.0",
    "date-fns": "2.0.0-alpha.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-winston": "^2.5.1",
    "le_node": "^1.7.1",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.4",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^23.6.0",
    "nock": "^9.3.2",
    "regenerator-runtime": "^0.13.1",
    "supertest": "^3.0.0"
  }
}
